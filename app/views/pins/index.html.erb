<h1>Index Page</h1>

<% @pins.each do |pin| %>
  <ul>
    <li>
      <%= pin.title %>
      <%= pin.description %>
      <%= link_to "show", pin_path(pin) %>
      <%= link_to "edit", edit_pin_path(pin) %>
      <%= link_to "Delete", pin_path(pin), method: :delete, data: {confirm: "Are you sure ?"} %>
    </li>
  </ul>
<% end %>

<%= link_to "Create New Pin", new_pin_path %>